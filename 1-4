input -> Misi mss
inputLower -> misi mss
hashtable ->
m 2
i 2
s 3
  1
  
public boolean isPalindrome(String input)
{
  Hashtable<Character,Integer> hashtable = new Hashtable<Character,Integer>();
  String inputLower = input.toLowerCase();
  for(int i=0; i<inputLower.length(); i++)
  {
    char c = inputLower.charAt(i);
    if(hashtable.containsKey(c))
      hashtable.put(c, hashtable.get(c)+1);
     else
      hashtable.put(c, 1);
  }
  
  Iterator<Character> itr = hashtable.getKeyIterator();
  int oddOccurrence = 0;
  for(Character c: itr.next())
  {
    if(hashtable.get(c)%2 != 0 && !c.isWhitespace())
    {
      oddOccurrence++;
      if(oddOccurence>1)
        return false;
    }
  }
  return true;
}
