// O(min(n, c))  c - num of characters


public bool hasUniqueChars(String str){
  HashSet<String> set = new HashSet<String>();
  for(int i=0; i<str.length(); i++)
  {
    Char char = str.charAt(i);
    if(set.contains(char))
    {
      return false;
    }
    set.add(char);
  }
  return true;
}


// O(nlogn)

Apple
elppA

public bool hasUniqueChars(String str){
  String sortedStr = mergeSort(str);
  for(int i=0; i<str.length()-1; i++)
  {
    if(sortedStr.charAt(i) == sortedStr.charAt(i))
      return false;
  }
  return true;
}
